# CC1101 SPI transport
SPI_DEVICE=/dev/spidev0.0
SPI_SPEED_HZ=50000

# RF behavior
REPEATS=5
CODE_BITS=18

# CAME command keys
TOGGLE_LIGHT=00 00 00 00 00 01 FE B5
FAN_UP=00 00 00 00 00 01 FE 97
FAN_DOWN=00 00 00 00 00 01 FE 90
FAN_OFF=00 00 00 00 00 01 FE 95

# Matter runtime
MATTER_STORAGE_FILE=/data/elica_rangehood_matter_storage.json
LOG_LEVEL=info
